          "notes": "From broker conversations"
        },
        {
          "cell": "C25",
          "label": "Broker's Name",
          "type": "text",
          "source": "external"
        },
        {
          "cell": "C26",
          "label": "Brokerage",
          "type": "text",
          "source": "external"
        },
        {
          "cell": "C27",
          "label": "Broker's Email",
          "type": "text",
          "source": "external"
        },
        {
          "cell": "C28",
          "label": "Broker's Phone Number",
          "type": "text",
          "source": "external"
        }
      ],
      "formulas": [
        {
          "cell": "B3",
          "formula": "=C8&\", \"&C9",
          "description": "City, State display",
          "depends_on": ["C8", "C9"]
        },
        {
          "cell": "I8",
          "formula": "=I6-I7",
          "description": "NOI calculation"
        },
        {
          "cell": "I11",
