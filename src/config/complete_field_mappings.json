          "json_path": "property.avg_unit_size"
        },
        {
          "cell": "F8",
          "label": "Current Vacancy",
          "type": "percentage",
          "source": "costar",
          "json_path": "vacancy.current.rate",
          "transform": "divide_by_100"
        },
        {
          "cell": "F9",
