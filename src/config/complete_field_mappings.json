          "source": "external",
          "notes": "Building count not in CoStar PDF"
        },
        {
          "cell": "F10",
          "label": "Floors",
          "type": "number",
          "source": "costar",
          "json_path": "property.stories"
        },
        {
          "cell": "I6",
          "label": "Trailing 12 Revenue",
          "type": "currency",
          "source": "external",
          "notes": "From property financials (OM or rent roll)"
        },
        {
          "cell": "I7",
          "label": "Trailing 12 Expenses",
          "type": "currency",
          "source": "external",
          "notes": "From property financials (OM or rent roll)"
        },
        {
          "cell": "I8",
          "label": "Trailing 12 NOI",
          "type": "currency",
          "source": "formula",
          "formula": "=I6-I7",
          "depends_on": ["I6", "I7"]
        },
        {
          "cell": "I10",
          "label": "Guidance Price",
          "type": "currency",
          "source": "external",
          "notes": "Deal-specific asking price"
        },
        {
          "cell": "I11",
          "label": "Implied Cap Rate",
          "type": "percentage",
          "source": "formula",
          "formula": "=IFERROR(I8/I10,\"\")",
          "depends_on": ["I8", "I10"]
        },
        {
          "cell": "I12",
          "label": "Price per Unit",
          "type": "currency",
          "source": "formula",
          "formula": "=IFERROR(I10/F6,\"\")",
          "depends_on": ["I10", "F6"]
        },
        {
          "cell": "I13",
          "label": "Call for Offers Date",
          "type": "date",
          "source": "external",
          "notes": "Deal-specific"
        },
        {
          "cell": "C19",
          "label": "Seller Name",
          "type": "text",
          "source": "costar",
          "json_path": "owner.name"
        },
        {
          "cell": "C20",
          "label": "How Long Have They Owned?",
          "type": "number",
          "format": "years",
          "source": "costar",
          "json_path": "owner.purchase_date",
          "transform": "years_since_purchase"
