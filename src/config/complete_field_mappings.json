          "json_field": "avgAskingRent.perUnit"
        },
        {
          "column": "I",
          "label": "Gap to Achieved",
          "type": "currency",
          "source": "formula",
          "formula": "=H{row}-J{row}"
        },
        {
          "column": "J",
          "label": "Average Effective Rent",
          "type": "currency",
          "source": "costar",
          "json_field": "avgEffectiveRent.perUnit"
        },
        {
          "column": "K",
          "label": "Price per SF",
          "type": "currency",
          "source": "costar",
