{
  "version": "2.0.0",
  "description": "Complete field mappings from Docling JSON extract to XLSX template cells - covers ALL sheets",
  "template_name": "Serafina UW Phoenix AZ Feb 14 2025",
  "json_root": "structured_data[0]",
  "last_updated": "2025-12-29",
  
  "source_types": {
    "costar": {
      "symbol": "âœ…",
      "description": "Available from Docling PDF extraction of CoStar report",
      "auto_fill": true
    },
    "external": {
      "symbol": "ðŸ”¶",
      "description": "Requires manual input or separate data source",
      "auto_fill": false,
      "highlight": true
    },
    "formula": {
      "symbol": "ðŸ“Š",
      "description": "Calculated from other cells - DO NOT OVERWRITE",
      "auto_fill": false,
      "protect": true
    },
    "cross_sheet": {
      "symbol": "ðŸ”—",
      "description": "References data from another sheet",
      "auto_fill": false
    }
  },
  
  "transforms": {
    "divide_by_100": {
      "description": "Convert percentage from whole number to decimal (6.0 -> 0.06)",
      "implementation": "value / 100"
    },
    "to_number": {
      "description": "Convert string to number, removing formatting ($27,450,000 -> 27450000)",
      "implementation": "parseFloat(value.replace(/[^0-9.-]/g, ''))"
    },
    "years_since_purchase": {
      "description": "Calculate years from purchase date string ('Purchased Mar 2019' -> 6)",
      "implementation": "Extract year and calculate difference from current year"
    },
    "extract_property_name": {
      "description": "Extract property name before unit count ('Serafina 183 Unit...' -> 'Serafina')",
      "implementation": "match(/^(.+?)\\s+\\d+\\s+Unit/)[1]"
    },
    "extract_city": {
      "description": "Extract city from location string ('Phoenix, Arizona - ...' -> 'Phoenix')",
      "implementation": "split(',')[0].trim()"
    },
    "extract_state_abbrev": {
      "description": "Convert full state name to abbreviation ('Arizona' -> 'AZ')",
      "implementation": "state_abbrev_map[state_name]"
    },
    "bed_bath_label": {
      "description": "Create unit type label from bed/bath ({bed: 2, bath: 1} -> '2B/1Ba')",
      "implementation": "`${bed}B/${bath}Ba`"
    },
    "calc_occupied": {
      "description": "Calculate occupied units from total and available",
      "implementation": "total_units - available_units"
    }
  },

  "sheets": {
    "Property Summary": {
      "description": "Core property information - primary summary view for decision makers",
      "tables": [
        {
          "name": "Property Description",
          "range": "B6:C16",
          "description": "Basic property identification and location"
        },
        {
          "name": "Property Size", 
          "range": "E6:F10",
          "description": "Physical characteristics"
        },
        {
          "name": "Purchase Metrics",
          "range": "H6:I13",
          "description": "Financial acquisition data"
        },
        {
          "name": "Seller Information",
          "range": "B19:C28",
          "description": "Seller details and broker contact"
        }
      ],
      "mappings": [
        {
          "cell": "C6",
          "label": "Property Name",
          "type": "text",
          "source": "costar",
          "json_path": "property.name",
          "fallback_path": "sections_raw[0].content[0]",
          "fallback_transform": "extract_property_name"
        },
        {
          "cell": "C7",
          "label": "Street Address",
          "type": "text",
          "source": "costar",
          "json_path": "property.address",
          "fallback_path": "sections_raw[0].header"
        },
        {
          "cell": "C8",
          "label": "City",
          "type": "text",
          "source": "costar",
          "json_path": "property.city",
          "fallback_path": "sections_raw[0].content[1]",
          "fallback_transform": "extract_city"
        },
        {
          "cell": "C9",
          "label": "State",
          "type": "text",
          "source": "costar",
          "json_path": "property.state",
          "fallback_path": "sections_raw[0].content[1]",
          "fallback_transform": "extract_state_abbrev"
        },
        {
          "cell": "C10",
          "label": "County",
          "type": "text",
          "source": "external",
          "notes": "Not available in CoStar PDF"
        },
        {
          "cell": "C11",
          "label": "Zip Code",
          "type": "number",
          "source": "external",
          "notes": "Not available in CoStar PDF - could potentially geocode from address"
        },
        {
          "cell": "C12",
          "label": "Land Area",
          "type": "number",
          "format": "acres",
          "source": "external",
          "notes": "Not in CoStar PDF"
        },
        {
          "cell": "C13",
          "label": "Year Built",
          "type": "number",
          "source": "costar",
          "json_path": "property.year_built"
        },
        {
          "cell": "C14",
          "label": "Year Renovated",
          "type": "text",
          "source": "external",
          "notes": "Not in CoStar PDF"
        },
        {
          "cell": "C15",
          "label": "Management Type",
          "type": "text",
          "source": "external",
          "validation": ["3rd Party Management", "Self Managed"],
          "notes": "Dropdown selection"
        },
        {
          "cell": "C16",
          "label": "Management Company",
          "type": "text",
          "source": "costar",
          "json_path": "property_manager.name"
        },
        {
